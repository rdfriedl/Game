            <string>///update light
light.x = phy_position_x;
light.y = phy_position_y;

light2.x = phy_position_x;
light2.y = phy_position_y;

var addedAngle = 0;
if(global.ui_menu_showing == false){
    if(keyboard_check(ord('S')) || keyboard_check(vk_down)){
        addedAngle = 55;
    }
    else if(keyboard_check(vk_up) || keyboard_check(ord('W'))){
        addedAngle = -55;
    }
    
    if(keyboard_check(vk_left) || keyboard_check(ord('A'))){
        faceDir = 180;
    }
    if(keyboard_check(vk_right) || keyboard_check(ord('D'))){
        faceDir = 0;
    }

    if(faceDir == 180){
        pointDir = faceDir - addedAngle;
    }
    else pointDir = faceDir + addedAngle;
}

//update light dir
light.dir += (pointDir - light.dir) / 5;
</string>
