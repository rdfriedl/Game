          <argument>
            <kind>1</kind>
            <string>///controls
image_index=0;

//dont move if the menu is open
if(global.ui_menu_showing == false &amp;&amp; timeline_index != player_die){
    var _y = y + (sprite_height/2+1)*sin(rad(global.gravity_dir));
    phy_rotation = global.gravity_dir-90;
    image_angle = global.gravity_dir-90;
    var onGround = false;
    if(
        collision_point(bbox_left+1,_y,wall_par,true,true) || collision_point(bbox_left+1,_y,collision_par,true,true) ||
        collision_point(bbox_right-1,_y,wall_par,true,true) || collision_point(bbox_right-1,_y,collision_par,true,true)
        ){
        onGround = true;
    }
    
    if(!onGround){
        //phy_linear_velocity_x = phy_linear_velocity_x / 10;
    }
    
    switch(global.gravity_dir){
        default:
        case 90:
        case 270:
            if(
                (keyboard_check(vk_right) || keyboard_check(ord('D'))) &amp;&amp; 
                !(
                    collision_point(bbox_right+1,y,wall_par,true,true) || 
                    collision_point(bbox_right+1,y+13,wall_par,true,true) || 
                    collision_point(bbox_right+1,y,collision_par,true,true)
                )
            ){
                //move
                if(onGround)
                    phy_linear_velocity_x = groundMoveSpeed;
                else
                    phy_linear_velocity_x = airMoveSpeed;
                
                //set the image
                if(global.gravity_dir == 90)
                    image_index=2;
                else
                    image_index=1;
            }
            else if(
                (keyboard_check(vk_left) || keyboard_check(ord('A'))) &amp;&amp; 
                !(
                    collision_point(bbox_left-1,y,wall_par,true,true) || 
                    collision_point(bbox_left-1,y+13,wall_par,true,true) || 
                    collision_point(bbox_left-1,y,collision_par,true,true))
                ){
                //move
                if(onGround)
                    phy_linear_velocity_x = -groundMoveSpeed;
                else
                    phy_linear_velocity_x = -airMoveSpeed;
                
                //set the image
                if(global.gravity_dir == 90)
                    image_index=1;
                else
                    image_index=2;
            }
            else{
                phy_linear_velocity_x = 0;
            }
            
            if(keyboard_check(vk_space) || keyboard_check(vk_numpad0)){
                if(onGround){
                    physics_apply_local_impulse(0,0,0,-jump_power);
                    _jump_times = 0;
                }
                else if(_jump_times &lt; jump_times &amp;&amp; _jump_times != -1){
                    physics_apply_local_impulse(0,0,0,-jump_hold_power);
                    _jump_times+=1;
                }
            }
            if(keyboard_check_released(vk_space) || keyboard_check_released(vk_numpad0)){
                _jump_times = -1;
            }
            break;
    }
}
</string>
