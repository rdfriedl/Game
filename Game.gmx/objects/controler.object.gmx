            <string>///pring global.power_grids to console
var ds_map = global.power_grids;
var size = ds_map_size(ds_map);
var first = ds_map_find_first(ds_map);
show_debug_message('power_grids: '+string(size) + ' id: '+string(global.power_grids))
var ds_map = global.power_grids;

for (var i = 0; i &lt; size; i++;){
    var value = ds_map_find_value(ds_map,first);
    
    if(ds_exists(value,ds_type_map)){
        var space = '     ';
        var sizeLvl2 = ds_map_size(value);
        show_debug_message(space+string(first) + ': '+string(sizeLvl2) + ' id: '+string(value));
        var firstLvl2 = ds_map_find_first(value);
        for(var k = 0; k &lt; sizeLvl2; k +=1){
            show_debug_message(space+space+string(firstLvl2)+': '+string(ds_map_find_value(value,firstLvl2)));
            firstLvl2 = ds_map_find_next(value, firstLvl2)
        }
    }
    else{
        show_debug_message(first + ': ' + value);
    }
    
    first = ds_map_find_next(ds_map, first);
}
</string>
